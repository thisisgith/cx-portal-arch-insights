{
	"$schema": "../../../node_modules/@apollo/ng-swagger-gen/ng-swagger-gen-schema.json",
	"swagger": "../../node_modules/@swagger/cx-portal/swagger/Search.yaml",
	"output": "src/lib/search",
	"outputCore": "../core",
	"prefix": "Search",
	"ignoreUnusedModels": true,
	"minParamsForContainer": 2,
	"sortParams": "desc",
	"defaultTag": "Search",
	"replaceTags": {
