{
	"$schema": "../../../node_modules/@apollo/ng-swagger-gen/ng-swagger-gen-schema.json",
	"swagger": "../../node_modules/@swagger/cx-portal/swagger/Contracts.yaml",
	"output": "src/lib/contracts",
	"outputCore": "../core",
	"prefix": "Contracts",
	"ignoreUnusedModels": true,
	"minParamsForContainer": 2,
	"sortParams": "desc",
	"defaultTag": "Contracts",
	"replaceTags": {
		"Contract API Services": "Contracts"
	},
	"defaultOperationIds": {
		"head": {
			"/api/customerportal/contracts/v1/products/coverages": "headContractsProductsCoverages"
		}
	},
	"modelsExportAs": {
		"ErrorResponse": "ContractsErrorResponse"
	},
	"removeStaleFiles": true,
	"modelIndex": true,
	"serviceIndex": true,
	"apiModule": true,
	"enumModule": true,
	"generateExamples": true,
	"camelCase": false
}