{
	"name": "persona-based-console",
	"version": "1.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"sync": "npx ngdeploy -vns --source='src'",
		"deploy": "npx ngdeploy dev --source='src'",
		"deploy:auto": "npx ngdeploy qa -vn --no-environment --source='src' --gzip --ng-build=\"--aot\"",
		"deploy:qa": "npx ngdeploy qa -vn --no-environment --source='src' --gzip --ng-build=\"--aot\"",
		"deploy:stage": "npx ngdeploy stage -vn --no-environment --source='src' --gzip --ng-build=\"--aot\"",
		"deploy:prod": "npx ngdeploy prod -vn --no-environment --source='src' --gzip --ng-build=\"--aot\"",
		"start:docs": "npx compodoc -s",
		"postinstall": "cp node_modules/@apollo/linting/HTML/defaults.json .htmlhintrc",
		"build": "ng build",
		"build:docs": "npx compodoc -p tsconfig.json -n \"persona-based-console Documentation\" -d documentation --disablePrivate --hideGenerator --unitTestCoverage coverage/persona-based-console/coverage-summary.json",
		"test": "ng test --source-map=true --browsers=ChromeHeadless --code-coverage",
		"test:ci": "npm run test -- --watch=false",
		"test:automation": "npx cypress run",
		"posttest": "npx istanbul report --include **/coverage-final.json json && node combine-coverage.js",
		"lint": "npx apolloLint lint"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "~7.2.0",
		"@angular/common": "~7.2.0",
		"@angular/compiler": "~7.2.0",
		"@angular/core": "~7.2.0",
		"@angular/forms": "~7.2.0",
		"@angular/platform-browser": "~7.2.0",
		"@angular/platform-browser-dynamic": "~7.2.0",
		"@angular/router": "~7.2.0",
		"core-js": "^2.5.4",
		"rxjs": "~6.3.3",
		"tslib": "^1.9.0",
		"zone.js": "~0.8.26"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.13.0",
		"@apollo/schematics": "^1.0.1",
		"@apollo/linting": "^1.0.2",
		"@apollo/configs": "^1.0.1",
		"@angular/cli": "~7.3.5",
		"@angular/compiler-cli": "~7.2.0",
		"@angular/language-service": "~7.2.0",
		"@types/node": "~8.9.4",
		"@types/jasmine": "^3.3.9",
		"@types/jasminewd2": "~2.0.3",
		"@cisco-ngx/cui-auth": "^7.0.0",
		"@cisco-ngx/cui-components": "^7.0.0",
		"@cisco-ngx/cui-pipes": "^7.0.0",
		"@cisco-ngx/cui-services": "^7.0.0",
		"@cisco-ngx/cui-utils": "^7.0.0",
		"cisco-ui": "~1.3.4",
		"codelyzer": "~4.5.0",
		"cypress": "3.1.5",
		"cypress-xpath": "^1.3.0",
		"eslint-plugin-cypress": "^2.2.1",
		"istanbul-api": "^2.1.1",
		"istanbul-lib-coverage": "^2.0.3",
		"ngdeploy": "^2.0.0",
		"protractor": "~5.4.0",
		"ts-node": "~7.0.0",
		"typescript": "~3.2.2"
	},
	"cisco": {
		"app-descriptor": {
			"appId": "@APP_ID@",
			"name": "persona-based-console",
			"description": "persona-based-console Application",
			"version": "@APP_VERSION@",
			"branch": "@APP_BRANCH@",
			"sha": "@APP_SHA@",
			"default": false,
			"iconUrl": "",
			"entry": "index.html",
			"context": [],
			"contactInfo": {}
		}
	}
}
