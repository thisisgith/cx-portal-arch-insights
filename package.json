{
	"name": "persona-based-console",
	"version": "1.0.0",
	"scripts": {
		"ng": "ng",
		"start": "npx ngdeploy dev local --no-validate --env-only --source='src' && ng serve",
		"sync": "npx ngdeploy -vns --source='src'",
		"deploy": "npx ngdeploy dev --source='src'",
		"deploy:auto": "npx ngdeploy qa -vn --source='src' --gzip --ng-build=\"--aot\"",
		"deploy:qa": "npx ngdeploy qa -vn --source='src' --gzip --ng-build=\"--aot\"",
		"deploy:stage": "npx ngdeploy stage -vn --source='src' --gzip --ng-build=\"--aot\"",
		"deploy:prod": "npx ngdeploy prod -vn --source='src' --gzip --ng-build=\"--aot\"",
		"start:docs": "npx compodoc -s",
		"postinstall": "cp node_modules/@apollo/linting/HTML/defaults.json .htmlhintrc",
		"build": "ng build",
		"build:docs": "npx compodoc -n \"persona-based-console Documentation\" -d /documentation --disablePrivate --hideGenerator --unitTestCoverage coverage/coverage-summary.json",
		"test": "ngdeploy --env-only --source='src' && ng test --source-map=true --browsers=ChromeHeadless --code-coverage",
		"test:ci": "npm run test -- --watch=false",
		"posttest": "npx istanbul report --include **/coverage-final.json json && combine-coverage.js",
		"lint": "npx apolloLint lint --type=* --path=*"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "~7.2.0",
		"@angular/common": "~7.2.0",
		"@angular/compiler": "~7.2.0",
		"@angular/core": "~7.2.0",
		"@angular/forms": "~7.2.0",
		"@angular/platform-browser": "~7.2.0",
		"@angular/platform-browser-dynamic": "~7.2.0",
		"@angular/router": "~7.2.0",
		"core-js": "^2.5.4",
		"rxjs": "~6.3.3",
		"tslib": "^1.9.0",
		"zone.js": "~0.8.26"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.13.0",
		"@apollo/schematics": "^1.0.0",
		"@apollo/linting": "^1.0.2",
		"@apollo/configs": "^1.0.1",
		"@angular/cli": "~7.3.5",
		"@angular/compiler-cli": "~7.2.0",
		"@angular/language-service": "~7.2.0",
		"@types/node": "~8.9.4",
		"@types/jasmine": "^3.3.9",
		"@types/jasminewd2": "~2.0.3",
		"@cisco-ngx/cui-auth": "^7.0.0",
		"@cisco-ngx/cui-components": "^7.0.0",
		"@cisco-ngx/cui-pipes": "^7.0.0",
		"@cisco-ngx/cui-services": "^7.0.0",
		"@cisco-ngx/cui-utils": "^7.0.0",
		"cisco-ui": "~1.3.4",
		"codelyzer": "~4.5.0",
		"ngdeploy": "^1.3.0",
		"protractor": "~5.4.0",
		"ts-node": "~7.0.0",
		"typescript": "~3.2.2"
	},
	"cisco": {
		"config": {
			"client": {
				"*": {
					"origin": "https://swtgdev-apollo-2.cisco.com",
					"production": false
				},
				"local": {
					"auth": {
						"tokenUrl": "https://swtgdev-apollo-2.cisco.com/ws/oauth/v3/token/cway/{INSERT_CLIENT_ID}",
						"accountUrl": "https://swtgdev-apollo-2.cisco.com/ws/account/v2/",
						"referUrl": "https://swtgdev-apollo-2.cisco.com/ws/oauth/v3/sso/"
					}
				},
				"dev": {},
				"auto": {
					"production": true
				},
				"qa": {
					"production": true
				},
				"stage": {
					"production": true
				},
				"prod": {
					"production": true
				}
			}
		},
		"app-descriptor": {
			"appId": "@APP_ID@",
			"name": "persona-based-console",
			"description": "persona-based-console Application",
			"version": "@APP_VERSION@",
			"branch": "@APP_BRANCH@",
			"sha": "@APP_SHA@",
			"default": false,
			"iconUrl": "",
			"entry": "index.html",
			"context": [],
			"contactInfo": {}
		}
	}
}
