{
	"name": "persona-based-console",
	"version": "1.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"start:dev": "./utils/scripts/select-dev-env.sh",
		"start:background": "npm start & npx wait-on http-get://localhost:4200",
		"start:docs": "npx compodoc -s",
		"build": "ng build --vendor-chunk",
		"build:dev": "ng build --aot --output-hashing none --baseHref / --vendor-chunk  && npm run gzip",
		"build:qa": "ng build -c cxportal-qa --aot --build-optimizer --optimization=true --baseHref / --vendor-chunk && npm run gzip",
		"build:stage": "ng build -c stage --aot --build-optimizer --optimization=true --baseHref / --vendor-chunk && npm run gzip",
		"build:tie": "ng build -c tie --aot --build-optimizer --optimization=true --baseHref / --vendor-chunk && npm run gzip",
		"build:s3": "ng build -c cxportal-lt --aot --build-optimizer --optimization=true --baseHref / --vendor-chunk && npm run gzip",
		"build:ci": "ng build --prod --build-optimizer",
		"build:sdp": "ng build sdp-api",
		"build:docs": "npx compodoc -p tsconfig.json -n \"Persona Based Console Documentation\" -d documentation --disablePrivate --hideGenerator --unitTestCoverage coverage/coverage-summary.json",
		"gzip": "node utils/gzip.js",
		"generate:sdp": "npm run generate:sdp:contracts && npm run generate:sdp:control-points && npm run generate:sdp:customer-users && npm run generate:sdp:diagnostics && npm run generate:sdp:email && npm run generate:sdp:inventory && npm run generate:sdp:networkdata && npm run generate:sdp:product-alerts && npm run generate:sdp:racetrack-info && npm run generate:sdp:search && npm run generate:sdp:solution-based-racetrack-view && npm run generate:sdp:entitlement && npm run generate:sdp:feedback",
		"generate:sdp:contracts": "cd projects/sdp-api && ng-swagger-gen --config ./configs/Contracts.json",
		"generate:sdp:control-points": "cd projects/sdp-api && ng-swagger-gen --config ./configs/ControlPoints.json",
		"generate:sdp:customer-users": "cd projects/sdp-api && ng-swagger-gen --config ./configs/CustomerUsers.json",
		"generate:sdp:diagnostics": "cd projects/sdp-api && ng-swagger-gen --config ./configs/Diagnostics.json",
		"generate:sdp:email": "cd projects/sdp-api && ng-swagger-gen --config ./configs/Email.json",
		"generate:sdp:inventory": "cd projects/sdp-api && ng-swagger-gen --config ./configs/Inventory.json",
		"generate:sdp:networkdata": "cd projects/sdp-api && ng-swagger-gen --config ./configs/NDGW.json",
		"generate:sdp:product-alerts": "cd projects/sdp-api && ng-swagger-gen --config ./configs/ProductAlerts.json",
		"generate:sdp:racetrack-info": "cd projects/sdp-api && ng-swagger-gen --config ./configs/RacetrackInfo.json",
		"generate:sdp:search": "cd projects/sdp-api && ng-swagger-gen --config ./configs/Search.json",
		"generate:sdp:solution-based-racetrack-view": "cd projects/sdp-api && ng-swagger-gen --config ./configs/SolutionBasedRacetrackView.json",
		"generate:sdp:entitlement": "cd projects/sdp-api && ng-swagger-gen --config ./configs/Entitlement.json",
		"generate:sdp:entitlement-wrapper": "cd projects/sdp-api && ng-swagger-gen --config ./configs/EntitlementWrapper.json",
		"generate:sdp:org-user": "cd projects/sdp-api && ng-swagger-gen --config ./configs/OrgUser.json",
		"generate:sdp:feedback": "cd projects/sdp-api && ng-swagger-gen --config ./configs/Feedback.json",
		"generate:sdp:partner": "cd projects/sdp-api && ng-swagger-gen --config ./configs/Partner.json",
		"stats:analyze": "webpack-bundle-analyzer ./dist/stats.json",
		"stats:build:prod": "ng build --prod --build-optimizer --stats-json --vendor-chunk",
		"stats:build:qa": "ng build -c cxportal-qa --aot --build-optimizer --optimization --stats-json --vendor-chunk --baseHref /",
		"stats:build:dev": "ng build --stats-json",
		"test": "ng test --source-map=true --code-coverage",
		"test:ci": "npm run test -- --watch=false --browsers=ChromeHeadless",
		"test:automation": "cypress run --config screenshotOnRunFailure=false,video=false",
		"test:automation:advisories": "npm run test:automation -- --spec=cypress/integration/advisories_spec.js",
		"test:automation:assets": "npm run test:automation -- --spec=cypress/integration/assets_spec.js",
		"test:automation:communities": "npm run test:automation -- --spec=cypress/integration/communities_spec.js",
		"test:automation:general": "npm run test:automation -- --spec=cypress/integration/general_spec.js",
		"posttest": "npx istanbul report --include **/coverage-final.json json && node utils/combine-coverage.js",
		"lint": "node utils/lint/index.js lint --path=\"src\"",
		"lint:sdp": "node utils/lint/index.js --path=\"projects\"",
		"lint:cypress": "node utils/lint/index.js --path=\"cypress\"",
		"lint:commit:message": "node utils/gitcommit",
		"cypress": "npx cypress open"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^8.2.11",
		"@angular/common": "^8.2.11",
		"@angular/compiler": "^8.2.11",
		"@angular/core": "^8.2.11",
		"@angular/forms": "^8.2.11",
		"@angular/platform-browser": "^8.2.11",
		"@angular/platform-browser-dynamic": "^8.2.11",
		"@angular/router": "^8.2.11",
		"@brightcove/player-loader": "^1.7.1",
		"cloudsso-token-auth": "0.0.1",
		"cloudsso-token-fetcher": "0.0.6",
		"core-js": "^2.6.10",
		"cronstrue": "^1.83.0",
		"d3": "^5.12.0",
		"jquery": "^3.4.1",
		"lodash-es": "4.17.11",
		"mapbox-gl": "^0.54.0",
		"ngx-mapbox-gl": "^3.3.1",
		"rxjs": "^6.5.3",
		"smoothscroll-polyfill": "^0.4.4",
		"tslib": "^1.9.0",
		"zone.js": "~0.9.1"
	},
	"devDependencies": {
		"@angular-builders/custom-webpack": "^8.4.1",
		"@angular-devkit/build-angular": "^0.800.6",
		"@angular-devkit/build-ng-packagr": "^0.800.6",
		"@angular/cli": "^8.3.12",
		"@angular/compiler-cli": "^8.2.11",
		"@angular/http": "^7.2.15",
		"@angular/language-service": "^8.2.11",
		"@babel/core": "^7.6.2",
		"@babel/preset-env": "^7.6.2",
		"@cisco-ngx/cui-auth": "^7.14.10",
		"@cisco-ngx/cui-components": "^7.14.10",
		"@cisco-ngx/cui-pipes": "^7.14.10",
		"@cisco-ngx/cui-services": "^7.14.10",
		"@cisco-ngx/cui-utils": "^7.14.10",
		"@compodoc/compodoc": "^1.1.11",
		"@cui-x-views/csc": "^2.0.1",
		"@cui-x-views/mock": "^1.2.1",
		"@cui-x/cui-x-components": "^7.3.0",
		"@cui-x/services": "^1.0.5",
		"@cui-x/utilities": "^1.0.0",
		"@cx-portal/metrics": "^1.0.10",
		"@cypress/webpack-preprocessor": "^4.0.4",
		"@swagger/cx-portal": "^2.0.52",
		"@swagger/ng-swagger-gen": "^1.0.0",
		"@types/jasmine": "^3.4.0",
		"@types/jasminewd2": "^2.0.6",
		"@types/luxon": "^1.15.2",
		"@types/mapbox-gl": "^0.51.10",
		"@types/node": "~8.9.4",
		"angular-highcharts": "^7.2.0",
		"async.eachoflimit": "^0.5.2",
		"babel-loader": "^8.0.6",
		"cisco-ui": "1.3.5",
		"codelyzer": "~4.5.0",
		"colors": "^1.4.0",
		"csc-upload": "^1.0.2",
		"cypress": "3.4.1",
		"cypress-file-upload": "^3.4.0",
		"eslint": "^6.4.0",
		"eslint-config-airbnb-base": "^14.0.0",
		"eslint-plugin-cypress": "^2.6.0",
		"eslint-plugin-import": "^2.18.2",
		"fs-extra": "^8.1.0",
		"highcharts": "^7.2.0",
		"htmlhint": "^0.11.0",
		"istanbul": "^0.4.5",
		"istanbul-api": "^2.1.7",
		"istanbul-lib-coverage": "^2.0.5",
		"jasmine": "^3.5.0",
		"jasmine-core": "^3.5.0",
		"jasmine-spec-reporter": "^4.2.1",
		"karma": "^4.4.1",
		"karma-chrome-launcher": "^3.1.0",
		"karma-coverage-istanbul-reporter": "^2.1.0",
		"karma-jasmine": "^2.0.1",
		"karma-jasmine-html-reporter": "^1.4.2",
		"karma-spec-reporter": "0.0.32",
		"luxon": "^1.19.2",
		"ng-bullet": "^1.0.3",
		"ng-inline-svg": "^9.0.1",
		"ng-packagr": "^5.7.0",
		"papaparse": "^4.4.0",
		"pos": "^0.4.2",
		"protractor": "~5.4.0",
		"pubsub-js": "^1.6.1",
		"stylelint": "^10.1.0",
		"stylelint-config-recommended": "^2.2.0",
		"stylelint-config-standard": "^18.3.0",
		"stylelint-scss": "^3.11.1",
		"ts-loader": "^6.1.2",
		"ts-node": "~7.0.0",
		"tsickle": "^0.35.0",
		"tslib": "^1.9.0",
		"tslint": "^5.20.0",
		"tslint-config-airbnb": "^5.11.2",
		"typescript": "~3.4.5",
		"wait-on": "^3.2.0",
		"webpack-bundle-analyzer": "^3.6.0",
		"yargs": "^13.3.0"
	},
	"cisco": {
		"app-descriptor": {
			"appId": "@APP_ID@",
			"name": "Persona Based Console",
			"description": "Persona Based Console Application",
			"version": "@APP_VERSION@",
			"branch": "@APP_BRANCH@",
			"sha": "@APP_SHA@",
			"default": false,
			"iconUrl": "",
			"entry": "index.html",
			"context": [],
			"contactInfo": {}
		}
	}
}
