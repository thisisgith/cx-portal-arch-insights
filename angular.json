{
	"$schema": "./node_modules/@angular/cli/lib/config/schema.json",
	"version": 1,
	"newProjectRoot": "projects",
	"projects": {
		"persona-based-console": {
			"schematics": {},
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:browser",
					"configurations": {
						"production": {
							"aot": true,
							"budgets": [
								{
									"maximumError": "5mb",
									"maximumWarning": "2mb",
									"type": "initial"
								}
							],
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.prod.ts"
								},
								{
									"replace": "src/app/components/header/headerImports.module.ts",
									"with": "src/app/components/header/headerImports.module.prod.ts"
								}
							],
							"buildOptimizer": true,
							"extractCss": true,
							"extractLicenses": true,
							"namedChunks": false,
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"vendorChunk": false
						},
						"stage:internal": {
							"aot": true,
							"budgets": [
								{
									"maximumError": "5mb",
									"maximumWarning": "2mb",
									"type": "initial"
								}
							],
							"fileReplacements": [
								{
									"replace": "src/environments/environment.ts",
									"with": "src/environments/environment.stgi.ts"
								}
							],
							"buildOptimizer": true,
							"extractCss": true,
							"extractLicenses": true,
							"namedChunks": false,
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"vendorChunk": false
						}
					},
					"options": {
						"assets": [
							"src/favicon.png",
							"src/assets"
						],
						"es5BrowserSupport": true,
						"index": "src/index.html",
						"main": "src/main.ts",
						"outputPath": "dist/",
						"polyfills": "src/polyfills.ts",
						"scripts": [],
						"styles": [
							"src/styles.scss"
						],
						"tsConfig": "src/tsconfig.app.json"
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "persona-based-console:build"
					}
				},
				"lint": {
					"builder": "@angular-devkit/build-angular:tslint",
					"options": {
						"exclude": [
							"**/node_modules/**"
						],
						"tsConfig": [
							"src/tsconfig.app.json",
							"src/tsconfig.spec.json"
						]
					}
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"configurations": {
						"production": {
							"browserTarget": "persona-based-console:build:production"
						}
					},
					"options": {
						"browserTarget": "persona-based-console:build"
					}
				},
				"test": {
					"builder": "@angular-devkit/build-angular:karma",
					"options": {
						"assets": [
							"src/favicon.png",
							"src/assets"
						],
						"karmaConfig": "src/karma.conf.js",
						"main": "src/test.ts",
						"polyfills": "src/polyfills.ts",
						"scripts": [],
						"styles": [
							"src/styles.scss"
						],
						"tsConfig": "src/tsconfig.spec.json",
						"codeCoverageExclude": [
							"./src/app/**/*-routing.module.ts"
						]
					}
				}
			},
			"prefix": "app",
			"projectType": "application",
			"root": "",
			"sourceRoot": "src"
		}
	},
	"cli": {
		"defaultCollection": "@apollo/schematics"
	},
	"defaultProject": "persona-based-console"
}
